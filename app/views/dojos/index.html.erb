
<style type="text/css">
	table, th, tr, th, td {
		border: 1px solid black;
	}
</style>
<h1>Listing All Dojos</h1>
<table>
	<tr>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
	</tr>
	
	<% @dojos.each do |dojo| %>

	<tr>
		<td><%= dojo.branch %></td>
		<td><%= dojo.street %></td>
		<td><%= dojo.city %></td>
		<td><%= dojo.state %></td>

	</tr>
	<% end %>	

</table>
<br>
<a href = "/dojos/new">New Dojo </a>

